    "@emotion/styled": "^11.10.5",
    "@mui/material": "^5.10.13",
    "@mui/styled-engine-sc": "^5.10.6",
    "@swc/helpers": "^0.4.13",
    "@testing-library/jest-dom": "^5.16.5",
